{
  "name": "FootStep",
  "description": "Test Version. This is a support extension for your daily work.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs", "bookmarks"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "48": "images/footmark_cat_red1_48.png"
    },
    "default_title": "FootStep",
    "default_popup": "main.html"
  },
  "icons": {
    "48": "images/footmark_cat_red1_48.png"
  },
  "options_page": "options.html"
}
